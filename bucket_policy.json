{
    "Version": "2012-10-17",
    "Id": "GetObjectPolicy",
    "Statement": [
        {
            "Sid": "GetObjectStatement",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::your_bucket_name/*"
        }
    ]
}